-- Scripts:

local function QWRXL_fake_script() -- KhepronScriptLoader.OpenAnimate 
	local script = Instance.new('LocalScript', KhepronScriptLoader)

	function transparent(TransparencyValue)
		--Main
		script.Parent.Main.BackgroundTransparency = TransparencyValue
		script.Parent.Main.TopBar.BackgroundTransparency = TransparencyValue
		script.Parent.Main.TopBar.Version.TextTransparency = TransparencyValue
		script.Parent.Main.TopBar.Close.TextTransparency = TransparencyValue
		script.Parent.Main.TopBar.Close.BackgroundTransparency = TransparencyValue
		script.Parent.Main.TopBar.Logo.ImageTransparency = TransparencyValue
		script.Parent.Main.LoadScript.BackgroundTransparency = TransparencyValue
		script.Parent.Main.LoadScript.TextTransparency = TransparencyValue
		script.Parent.Main.GameText.TextTransparency = TransparencyValue
		--SupportedGames
		script.Parent.Main.SupportedGames.BackgroundTransparency = TransparencyValue
		script.Parent.Main.SupportedGames.TopBar.BackgroundTransparency = TransparencyValue
		script.Parent.Main.SupportedGames.TopBar.GamesHolder.BackgroundTransparency = TransparencyValue
		script.Parent.Main.SupportedGames.TopBar.GamesHolder.Games.TextTransparency = TransparencyValue
		script.Parent.Main.SupportedGames.TopBar.SupportedGamesLabel.TextTransparency = TransparencyValue
		--Info
		script.Parent.Main.Info.BackgroundTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.BackgroundTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.InformationLabel.TextTransparency = TransparencyValue
		--Info -Discord
		script.Parent.Main.Info.TopBar.Discord.BackgroundTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Discord.CopyInvite.BackgroundTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Discord.CopyInvite.TextTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Discord.DiscordInfo.TextTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Discord.DiscordLabel.TextTransparency = TransparencyValue
		--Info -Credits
		script.Parent.Main.Info.TopBar.Credits.BackgroundTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Credits.CreditsLabel.TextTransparency = TransparencyValue
		script.Parent.Main.Info.TopBar.Credits.Credits.TextTransparency = TransparencyValue
		wait(0.03)
		end
		transparent(1)
		transparent(0.9)
		transparent(0.8)
		transparent(0.7)
		transparent(0.6)
		transparent(0.5)
		transparent(0.4)
		transparent(0.3)
		transparent(0.2)
		transparent(0.1)
		transparent(0)
end
coroutine.wrap(QWRXL_fake_script)()
local function QGWP_fake_script() -- KhepronScriptLoader.LoadAnimate 
	local script = Instance.new('LocalScript', KhepronScriptLoader)

	function OnClicked()
		function transparent(TransparencyValue)
			--Main
			script.Parent.Main.BackgroundTransparency = TransparencyValue
			script.Parent.Main.TopBar.BackgroundTransparency = TransparencyValue
			script.Parent.Main.TopBar.Version.TextTransparency = TransparencyValue
			script.Parent.Main.TopBar.Close.TextTransparency = TransparencyValue
			script.Parent.Main.TopBar.Close.BackgroundTransparency = TransparencyValue
			script.Parent.Main.TopBar.Logo.ImageTransparency = TransparencyValue
			script.Parent.Main.LoadScript.BackgroundTransparency = TransparencyValue
			script.Parent.Main.LoadScript.TextTransparency = TransparencyValue
			script.Parent.Main.GameText.TextTransparency = TransparencyValue
			--SupportedGames
			script.Parent.Main.SupportedGames.BackgroundTransparency = TransparencyValue
			script.Parent.Main.SupportedGames.TopBar.BackgroundTransparency = TransparencyValue
			script.Parent.Main.SupportedGames.TopBar.GamesHolder.BackgroundTransparency = TransparencyValue
			script.Parent.Main.SupportedGames.TopBar.GamesHolder.Games.TextTransparency = TransparencyValue
			script.Parent.Main.SupportedGames.TopBar.SupportedGamesLabel.TextTransparency = TransparencyValue
			--Info
			script.Parent.Main.Info.BackgroundTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.BackgroundTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.InformationLabel.TextTransparency = TransparencyValue
			--Info -Discord
			script.Parent.Main.Info.TopBar.Discord.BackgroundTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Discord.CopyInvite.BackgroundTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Discord.CopyInvite.TextTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Discord.DiscordInfo.TextTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Discord.DiscordLabel.TextTransparency = TransparencyValue
			--Info -Credits
			script.Parent.Main.Info.TopBar.Credits.BackgroundTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Credits.CreditsLabel.TextTransparency = TransparencyValue
			script.Parent.Main.Info.TopBar.Credits.Credits.TextTransparency = TransparencyValue
			wait(0.03)
		end
		transparent(0.1)
		transparent(0.2)
		transparent(0.3)
		transparent(0.4)
		transparent(0.5)
		transparent(0.6)
		transparent(0.7)
		transparent(0.8)
		transparent(0.9)
		transparent(1)
		script.Parent:Destroy()
	end 
	script.Parent.Main.LoadScript.MouseButton1Up:Connect(OnClicked)
end
coroutine.wrap(QGWP_fake_script)()
